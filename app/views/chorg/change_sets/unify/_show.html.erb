<dl class="see after_move">
  <dt><%= t "chorg.views.#{params[:type]}_change_sets.after_unify" %></dt>
  <dd></dd>
  <% Chorg::ChangeSet::GROUP_ATTRIBUTES.each do |k| %>
    <dt><%= t "chorg.views.#{params[:type]}_change_sets.#{k}" %></dt>
    <dd><%= @item.destinations.try(:first).try(:send, :[], k) %></dd>
  <% end %>
</dl>

<dl class="see before_move">
  <dt><%= t "chorg.views.#{params[:type]}_change_sets.before_unify" %></dt>
  <dd></dd>
  <dt><%= t "chorg.views.#{params[:type]}_change_sets.name" %></dt>
  <dd><%= br @item.sources.map { |e| e["name"] }.join("\n") %></dd>
</dl>
